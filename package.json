{
  "name": "kubernetes-github-example",
  "version": "1.0.0",
  "description": "This is an example project showing how to integrate GitHub with a Kubernetes cluster running on AWS EKS.",
  "main": "server.js",
  "scripts": {
    "test": "npm run container-test-functional && npm run container-test-e2e",
    "container-test-functional": "node ./scripts/test-functional.js",
    "container-test-e2e": "node ./scripts/test-e2e.js",
    "local-test": "npm run local-test-functional && npm run local-test-e2e",
    "local-test-functional": "jest",
    "local-test-e2e": "node ./scripts/local-test-e2e.js",
    "start": "node ./src/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinswiber/kubernetes-github-example.git"
  },
  "keywords": [],
  "author": "Kevin Swiber <kswiber@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevinswiber/kubernetes-github-example/issues"
  },
  "homepage": "https://github.com/kevinswiber/kubernetes-github-example#readme",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^27.4.5",
    "newman": "^5.3.0",
    "superagent": "^6.1.0",
    "supertest": "^6.1.6"
  }
}
